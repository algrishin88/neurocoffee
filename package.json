{
  "name": "neirocafe-backend",
  "version": "1.0.0",
  "description": "Backend сервер для НейроКофейни",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:3001": "node scripts/start-on-port.js 3001",
    "start:3308": "node scripts/start-on-port.js 3308",
    "dev": "nodemon server.js",
    "db:init": "node scripts/initPostgres.js && node scripts/initMenu.js",
    "db:migrate": "node scripts/migrate-orders-payment.js",
    "db:migrate-profile": "node scripts/migrate-users-profile.js",
    "db:migrate-newsletter": "node scripts/migrate-newsletter.js",
    "db:migrate-bookings-zone": "node scripts/migrate-bookings-zone.js",
    "setup": "npm install && npm run db:init",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cafe",
    "coffee",
    "api"
  ],
  "author": "Trogovitsky",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^8.0.1",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
